{
    "compilerOptions": {
        "module": "None",
        "target": "ES5",
        "outDir": "dist",
        
        /* Strict */
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitOverride": true
    }
}